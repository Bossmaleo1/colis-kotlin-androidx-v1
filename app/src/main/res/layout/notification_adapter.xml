<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/user_first_name93"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/graycolor"
        android:padding="10dp"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/background_menu_message_public"
            android:layout_gravity="center"
            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/block1"
                >


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="13dp"
                    android:id="@+id/iconetmachin"
                    >

                    <com.facebook.drawee.view.SimpleDraweeView
                        android:id="@+id/icon"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="4dp"
                        android:src="@drawable/ic_profile_colorier"
                        fresco:roundedCornerRadius="20dp"
                        />

                    <LinearLayout
                        android:id="@+id/block93"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_toRightOf="@id/icon"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="4dp"
                        >

                        <TextView
                            android:id="@+id/title93"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:tooltipText="Consulter le profil du demandeur"
                            />

                        <TextView
                            android:id="@+id/title1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:textSize="10dp"
                            />
                    </LinearLayout>

                </RelativeLayout>


                <View
                    android:layout_below="@+id/iconetmachin"
                    android:id="@+id/viewbis3"
                    android:layout_width="match_parent"
                    android:layout_height="6dip"
                    android:layout_marginLeft="4dip"
                    android:layout_marginRight="4dip"
                    android:background="@android:color/transparent"
                    />


                <View
                    android:layout_below="@+id/viewbis3"
                    android:id="@+id/ville_depart"
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="@color/graycolor"
                    />



                <LinearLayout
                    android:id="@+id/ville_depart_block"
                    android:layout_below="@+id/ville_depart"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="5dp"
                    >

                    <ImageView
                        android:id="@+id/icon_message93"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/baseline_speaker_notes_black_24"
                        android:tint="@color/colorPrimary"
                        android:layout_gravity="center"
                        />



                    <TextView
                        android:id="@+id/contenu93"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_gravity="center"
                        />


                </LinearLayout>


                <LinearLayout
                    android:id="@+id/heure_depart_block93"
                    android:layout_below="@+id/ville_depart_block"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="5dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/baseline_local_parking_black_24"
                        android:tint="@color/colorPrimary"
                        android:layout_gravity="center"
                        />



                    <TextView
                        android:id="@+id/contenu_ville_arrivee_block93"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_gravity="center"
                        />


                </LinearLayout>


                <LinearLayout
                    android:id="@+id/telephone_block"
                    android:layout_below="@+id/heure_depart_block93"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="5dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/baseline_call_black_24"
                        android:tint="@color/colorPrimary"
                        android:layout_gravity="center"
                        />



                    <TextView
                        android:id="@+id/telephone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_gravity="center"
                        />


                </LinearLayout>



                <LinearLayout
                    android:id="@+id/heure_arrivee_block93"
                    android:layout_below="@+id/telephone_block"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="5dp"
                    >

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/send"
                        android:layout_below="@+id/nom_proprietaire_block"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="VALIDER"
                        android:background="@color/colorPrimary"
                        android:layout_marginTop="10dp"
                        android:textColor="@android:color/white"
                        />

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/send2"
                        android:layout_below="@+id/nom_proprietaire_block"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:text="ANNULER"
                        app:backgroundTint="@color/colorError"
                        app:backgroundTintMode="@color/colorError"
                        android:layout_marginTop="10dp"
                        android:textColor="@android:color/white"
                        />


                </LinearLayout>

                <View
                    android:layout_below="@+id/heure_arrivee_block93"
                    android:id="@+id/trace_separation93"
                    android:layout_width="match_parent"
                    android:layout_height="16dip"
                    android:layout_marginLeft="4dip"
                    android:layout_marginRight="4dip"
                    android:background="@android:color/white"
                    />

                <View
                    android:layout_below="@+id/trace_separation93"
                    android:id="@+id/viewbis1893"
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="@color/graycolor"
                    />

                <View
                    android:layout_below="@+id/viewbis1893"
                    android:id="@+id/trace_separation293"
                    android:layout_width="match_parent"
                    android:layout_height="16dip"
                    android:layout_marginLeft="4dip"
                    android:layout_marginRight="4dip"
                    android:background="@android:color/white"
                    />

                <View
                    android:layout_below="@+id/ville_depart_block"
                    android:id="@+id/trace_separation_gone"
                    android:layout_width="match_parent"
                    android:layout_height="16dip"
                    android:layout_marginLeft="4dip"
                    android:layout_marginRight="4dip"
                    android:background="@android:color/white"
                    android:visibility="gone"
                    />

                <View
                    android:layout_below="@+id/trace_separation_gone"
                    android:id="@+id/viewbis18_gone"
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="@color/graycolor"
                    android:visibility="gone"
                    />

                <View
                    android:layout_below="@+id/viewbis18"
                    android:id="@+id/trace_separation2_gone"
                    android:layout_width="match_parent"
                    android:layout_height="16dip"
                    android:layout_marginLeft="4dip"
                    android:layout_marginRight="4dip"
                    android:background="@android:color/white"
                    android:visibility="gone"
                    />


            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>
</androidx.cardview.widget.CardView>